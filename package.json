{
  "name": "playground",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://marcusnog.github.io/playground-app-prd",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
